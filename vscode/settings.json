{
    "typescript.tsdk": "node_modules/typescript/lib",
    "files.associations": {
        "*.js": "typescriptreact",
        "*.ts": "typescriptreact"
    },
    "files.autoSave": "off",
    "editor.multiCursorModifier": "alt",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    }
}